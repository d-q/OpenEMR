@model IEnumerable<OpenEMR.Models.Tindakan>

@{
    ViewData["Title"] = "Tindakan";
}

<h1>Tindakan</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pemeriksa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kunjungan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TindakanMedis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WaktuPemeriksaan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pemeriksa.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Kunjungan.PasienDanNomorRegis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TindakanMedis.KategoriPemeriksaan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WaktuPemeriksaan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
