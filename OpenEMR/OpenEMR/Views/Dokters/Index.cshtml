@model IEnumerable<OpenEMR.Models.Dokter>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomorSIP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TaripPemeriksaan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Poli)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NIK)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TanggalLahir)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TempatLahir)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JenisKelamin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AlamatRumah)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomorTeleponPribadi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomorTeleponDarurat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AlamatEmailPribadi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StatusPernikahan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomorSIP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TaripPemeriksaan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Poli.NamaPoli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NIK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TanggalLahir)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TempatLahir)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JenisKelamin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AlamatRumah)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomorTeleponPribadi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomorTeleponDarurat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AlamatEmailPribadi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StatusPernikahan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
